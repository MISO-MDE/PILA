                        </ifSentence>
                      </elseSentence>
                    </formulaBody>
                  </ifSentence>
                </elseSentence>
              </formulaBody>
              <formulaBody>
                <ifSentence>
                  <logExp>
                    <variable variable="//@intermediary/@superentities.0/@participants.0/@attLists.5"/>
                    <logOper>=</logOper>
                    <variable2 valor="0"/>
